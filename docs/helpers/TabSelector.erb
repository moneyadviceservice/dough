<%=
  tab_selector :section_name do |tab|
    tab.section do |container|
      container.active
      container.heading 'Tab one'
      container.content do
        'Tab one'
      end
    end

    tab.section do |container|
      container.heading 'Tab two'
      container.content do
        'Tab two'
      end
    end
  end
%>
